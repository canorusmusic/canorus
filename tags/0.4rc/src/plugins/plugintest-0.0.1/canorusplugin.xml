<plugin>
	<canorus-version>0.2.0</canorus-version>
	<name>plugintest</name>
	<description>Plugintest</description>
	<version>0.0.1</version>
	<author>Canorus team</author>
	<description lang="sl_SI">Testni vtičnik</description>

	<home-url>http://www.canorus.org</home-url>
	<update-url>http://</update-url>

	<action>
		<name>pluginTestInit</name>
		<on-action>onInit</on-action>
		<lang>python</lang>
		<filename>init.py</filename>
		<function>init</function>
	</action>

	<action>
		<name>pluginTestExportFunctionMarkings</name>
		<export-filter>Plugin test files (*.test)</export-filter>
		<export-filter lang="sl_SI">Testni vtičnik (*.test)</export-filter>
		<lang>python</lang>
		<filename>export.py</filename>
		<function>export</function>
		<args>export-filename</args>
	</action>
	
	<menu>
		<name>pluginTestHarmonyAnalysisMenu</name>
		<title>Plugin test</title>
		<title lang="sl_SI">Testni vtičnik</title>
	</menu>
	
	<action>
		<parent-menu>pluginTestHarmonyAnalysisMenu</parent-menu>
		<name>pluginTestAnalyseScore</name>
		<text>Plugin test analyse</text>
		<text lang="sl_SI">Testno analiziraj trenutni list</text>
		<lang>python</lang>
		<filename>analysis.py</filename>
		<function>analyse</function>
		<args>sheet</args>
		<refresh/>
	</action>
</plugin>
